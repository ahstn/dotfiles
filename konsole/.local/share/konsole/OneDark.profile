[Appearance]
ColorScheme=OneDark
Font=Hack,12,-1,5,50,0,0,0,0,0,Regular
UseFontLineChararacters=true

[General]
Command=/bin/zsh
Environment=TERM=xterm-256color,COLORTERM=truecolor
Icon=/dev/null/
LocalTabTitleFormat=%d
Name=OneDark
Parent=FALLBACK/
ShowTerminalSizeHint=false
TerminalColumns=180
TerminalRows=90

[Interaction Options]
AutoCopySelectedText=true
MiddleClickPasteMode=1
UnderlineLinksEnabled=false

[Scrolling]
ScrollBarPosition=2

[Terminal Features]
BidiRenderingEnabled=false
BlinkingCursorEnabled=false
BlinkingTextEnabled=false
UrlHintsModifiers=33554432
